 \"admin\",\r\n      password: \"123456\",\r\n      model: \"\",\r\n      fullscreenLoading: false,\r\n    };\r\n  },\r\n  methods: {\r\n    loginDo() {\r\n      this.fullscreenLoading = true;\r\n      this.$http\r\n        .post(\"/api/login.do\", {\r\n          userName: this.userName,\r\n          password: this.password,\r\n        })\r\n        .then((response) => {\r\n          this.fullscreenLoading = false;\r\n          if (response.data.code == 200) {\r\n            this.$Cookies.set(\"token\", response.data.result);\r\n            this.$Cookies.set(\"userName\", this.userName);\r\n            this.$message({\r\n              message: response.data.msg,\r\n              type: \"success\",\r\n              center: true,\r\n            });\r\n            this.$router.push({\r\n              path: \"/admin/\",\r\n            });\r\n          } else {\r\n            this.$message({\r\n              message: response.data.msg,\r\n              type: \"error\",\r\n              center: true,\r\n            });\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          this.fullscreenLoading = false;\r\n          console.log(error);\r\n          this.$message({\r\n            message: \"服务器内部错误\",\r\n            type: \"error\",\r\n            center: true,\r\n          });\r\n        });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.$Cookies.remove(\"token\");\r\n    this.$Cookies.remove(\"userName\");\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.login {\r\n  background-image: url(../assets/1.jpg);\r\n  background-size: 100% 100%;\r\n  margin: 0px;\r\n  padding: 0px;\r\n  height: 100%;\r\n}\r\n.el-form {\r\n  width: 400px;\r\n  height: 400px;\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  margin: auto;\r\n  text-align: center;\r\n}\r\n.el-input {\r\n  margin: 10px;\r\n}\r\n</style>\r\n"]}]}